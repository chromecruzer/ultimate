{
  "name": "my_rust_lib",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "./my_rust_lib/src",
  "targets": {
    "build": {
      "executor": "@monodon/rust:check",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/my_rust_lib"
      }
    },
    "test": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/my_rust_lib"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/my_rust_lib"
      }
    }
  },
  "tags": []
}
